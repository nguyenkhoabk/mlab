  }


  Serial.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;Initializing SD card...&quot;&lt;/span&gt;);

  &lt;span style=&quot;color: #007020; font-weight: bold&quot;&gt;if&lt;/span&gt; (&lt;span style=&quot;color: #666666&quot;&gt;!&lt;/span&gt;SD.begin(&lt;span style=&quot;color: #40a070&quot;&gt;3&lt;/span&gt;)) {
