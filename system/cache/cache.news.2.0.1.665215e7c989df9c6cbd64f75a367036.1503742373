    myFile.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;/&quot;&lt;/span&gt;);
    myFile.print(now.month());
    myFile.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;/&quot;&lt;/span&gt;);
    myFile.print(now.year());
    myFile.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;  &quot;&lt;/span&gt;);
    myFile.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;Thoi diem: &quot;&lt;/span&gt;);
    myFile.print(now.hour());
    myFile.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;:&quot;&lt;/span&gt;);
    myFile.print(time_minute);
    myFile.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;:&quot;&lt;/span&gt;);
    myFile.print(time_second);
    myFile.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;  &quot;&lt;/span&gt;);
    myFile.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;Nhiet do: &quot;&lt;/span&gt;);
    myFile.print(t);
    myFile.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;*C &quot;&lt;/span&gt;);
    myFile.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;Do am: &quot;&lt;/span&gt;);
    myFile.print(h);
    myFile.println(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;%&quot;&lt;/span&gt;);
    myFile.close();
    }
      &lt;span style=&quot;color: #60a0b0; font-style: italic&quot;&gt;//đóng file :&lt;/span&gt;
      myFile.close();
