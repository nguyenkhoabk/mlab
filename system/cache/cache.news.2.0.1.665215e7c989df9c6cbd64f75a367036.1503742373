&lt;span style=&quot;color: #902000&quot;&gt;void&lt;/span&gt; &lt;span style=&quot;color: #06287e&quot;&gt;setup&lt;/span&gt; () {
  &lt;span style=&quot;color: #007020; font-weight: bold&quot;&gt;while&lt;/span&gt; (&lt;span style=&quot;color: #666666&quot;&gt;!&lt;/span&gt;Serial); &lt;span style=&quot;color: #60a0b0; font-style: italic&quot;&gt;// for Leonardo/Micro/Zero&lt;/span&gt;

  Serial.begin(&lt;span style=&quot;color: #40a070&quot;&gt;57600&lt;/span&gt;);
  &lt;span style=&quot;color: #007020; font-weight: bold&quot;&gt;if&lt;/span&gt; (&lt;span style=&quot;color: #666666&quot;&gt;!&lt;/span&gt; rtc.begin()) {
    Serial.println(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;Couldn't find RTC&quot;&lt;/span&gt;);
    &lt;span style=&quot;color: #007020; font-weight: bold&quot;&gt;while&lt;/span&gt; (&lt;span style=&quot;color: #40a070&quot;&gt;1&lt;/span&gt;);
  }
  rtc.adjust(DateTime(F(__DATE__), F(__TIME__))); 
  
  DateTime now &lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt; rtc.now();
    
  Serial.print(now.year(), DEC);
  Serial.print(&lt;span style=&quot;color: #4070a0&quot;&gt;'/'&lt;/span&gt;);
  Serial.print(now.month(), DEC);
  Serial.print(&lt;span style=&quot;color: #4070a0&quot;&gt;'/'&lt;/span&gt;);
  Serial.print(now.day(), DEC);
  Serial.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot; (&quot;&lt;/span&gt;);
  Serial.print(daysOfTheWeek[now.dayOfTheWeek()]);
  Serial.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;) &quot;&lt;/span&gt;);
  Serial.print(now.hour(), DEC);
  Serial.print(&lt;span style=&quot;color: #4070a0&quot;&gt;':'&lt;/span&gt;);
    
  &lt;span style=&quot;color: #902000&quot;&gt;int&lt;/span&gt; time_minute &lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt; now.minute();
  &lt;span style=&quot;color: #902000&quot;&gt;int&lt;/span&gt; time_second &lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt; now.second(); 
  &lt;span style=&quot;color: #902000&quot;&gt;int&lt;/span&gt; time_ok &lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt; &lt;span style=&quot;color: #40a070&quot;&gt;0&lt;/span&gt;;
    
  &lt;span style=&quot;color: #007020; font-weight: bold&quot;&gt;if&lt;/span&gt;(time_second &lt;span style=&quot;color: #666666&quot;&gt;&amp;gt;=&lt;/span&gt; (&lt;span style=&quot;color: #40a070&quot;&gt;60&lt;/span&gt; &lt;span style=&quot;color: #666666&quot;&gt;-&lt;/span&gt; time_error)){
    time_ok &lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt; time_error &lt;span style=&quot;color: #666666&quot;&gt;+&lt;/span&gt; time_second &lt;span style=&quot;color: #666666&quot;&gt;-&lt;/span&gt; &lt;span style=&quot;color: #40a070&quot;&gt;60&lt;/span&gt;;
    Serial.print(time_minute &lt;span style=&quot;color: #666666&quot;&gt;+&lt;/span&gt; &lt;span style=&quot;color: #40a070&quot;&gt;1&lt;/span&gt;);
    Serial.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;:&quot;&lt;/span&gt;);
    Serial.print(time_ok);
  }
  &lt;span style=&quot;color: #007020; font-weight: bold&quot;&gt;else&lt;/span&gt; {
    Serial.print(now.minute());
    Serial.print(&lt;span style=&quot;color: #4070a0&quot;&gt;&quot;:&quot;&lt;/span&gt;);
    Serial.print(time_second &lt;span style=&quot;color: #666666&quot;&gt;+&lt;/span&gt; time_error);
  }
  Serial.println();
  delay(&lt;span style=&quot;color: #40a070&quot;&gt;1000&lt;/span&gt;);
}

